/* styles for v5+ browsers */

body {
	margin:0;
	padding:0;
	color:#666;
	background:#fff;
	font:76% Arial, Times New Roman, Times, serif,Georgia;
	}

/* display elements */
h1, h2, h3, p {
	margin:.5em 0;
	padding:0;
	}
h1 {
	font-size:1.6em;
	}
h2 {
	font-size:1.4em;color:#333;
	}
h3 {
	font-size:1.2em;
	border-bottom:1px solid #333;
	}
p{
	font-size:1.1em;
	}
a {
	/*border-bottom:1px solid #1f79ac;*/
	text-decoration: underline;
	color:#999;
	}
a:hover {
	color:#999;
	}
.error {
	color:#1f79ac;
	background:#fff;
	}
.bold {
	font-weight:bold;
	}
.normal {
	font-weight:normal;
	}
/* list elements */
ul {
	margin:0;
	padding:0;
	list-style:none;
	}
ul.bullet li{padding:2px;list-style: disc outside;margin:0 18px;}

/* admin elements */
#admin, #admin a {
	margin:0;
	padding:.5em 0;
	background:#f00;
	color:#fff;
	}

/* hidden elements */
.hide {
	display:none;
	}

.left {
	float:left;
	}
.right {
	float:right;
	}
.clear-all { /* can be used on a <div> or <br> to avoid collapsing divs - especially ns6 (pc) */
	clear:both;
	height:0;
	line-height:0;
	font-size:0;
	}

/* structural elements */
/* if no right column required, remove #col-three and #col-two then put content in #col-two-three */
#wrapper {
	width:830px; /* #wrapper = #col-one + 10px + #col-two-three */
	background:#fff;
	}
#header {
	width:830px;
	height:96px;
	margin:0px;
	padding:0;
	background:#fff;
	}
#col-one {
	width:180px;
	float:left;
	margin:0 20px 0 0;
	background:#fff;
	}
#col-two-three { /* #col-two-three = #col-two + 10px + #col-three */
	width:630px;
	float:right;
	margin:0;
	padding:0;	
	background:#fff;
	}
#col-two {
	width:630px;
